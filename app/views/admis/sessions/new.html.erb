<div class="container">
<h1 class="log1">Acceso de Administrador</h1>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <%= f.input :email, required: false, autofocus: true, label: "Correo de Administrador" %>
    <%= f.input :password, required: false, label: "ContraseÃ±a" %>
    <%= f.input :remember_me, label: "Recordarme", as: :boolean if devise_mapping.rememberable? %>
  </div>

  <div class="form-actions">
    <p class="log2"><%= f.button :submit, "Ingresar" %></p>
  </div>
<% end %>

<div class="container">
<%= render "admis/shared/links" %>
</div>
</div>