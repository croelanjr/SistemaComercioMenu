<%- if controller_name != 'sessions' %>
  <p class="log2"><%= link_to "Ingresar", new_session_path(resource_name) %><br /></p>
<% end -%>

<!-- <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrar", new_registration_path(resource_name) %><br />
<% end -%> -->

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <p class="log2"><%= link_to "Olvidó tu contraseña?", new_password_path(resource_name) %><br /></p>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <p class="log2"><%= link_to "No recibistes Instruciones de confirmación?", new_confirmation_path(resource_name) %><br /></p>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <p class="log2"><%= link_to "No recibistes Instruciones de Desbloqueo?", new_unlock_path(resource_name) %><br /></p>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <p class="log2"><%= link_to "Ingresado con #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider) %><br /></p>
  <% end -%>
<% end -%>
